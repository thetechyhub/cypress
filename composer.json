{
    "name": "thetechyhub/cypress",
    "description": "Laravel Cypress Boilerplate",
    "keywords": [
        "laracasts",
        "cypress"
    ],
    "homepage": "https://github.com/thetechyhub/cypress",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jeffrey Way",
            "email": "jeffrey@laracasts.com",
            "role": "Developer"
        },
        {
            "name": "AIMEN.S.A.SASI",
            "email": "aimensasi@icloud.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laracasts\\Cypress\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laracasts\\Cypress\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laracasts\\Cypress\\CypressServiceProvider"
            ]
        }
    }
}
